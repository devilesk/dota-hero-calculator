{
  "name": "dota-hero-calculator",
  "version": "1.0.0",
  "private": true,
  "description": "Dota 2 Hero Calculator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devilesk/dota-hero-calculator.git"
  },
  "author": "devilesk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devilesk/dota-hero-calculator/issues"
  },
  "scripts": {
    "build": "browserify -d -t brfs -x 'knockout' -x 'jquery' www/js/main.js > www/js/bundle.js",
    "build-prod": "browserify -d -t brfs -x 'knockout' -x 'jquery' -p [minifyify --map bundle.js.map --output dist/js/bundle.js.map] www/js/main.js > dist/js/bundle.js",
    "testbuild": "browserify -d -t brfs -p [minifyify --map test2.bundle.js.map --output www/js/test2.bundle.js.map] www/js/test2.js > www/js/test2.bundle.js",
    "standalonebuild": "browserify -d -x 'knockout' -x 'jquery' --standalone HeroCalc www/js/herocalc/main.js > www/js/herocalc.js",
    "tablebuild": "browserify -d -t brfs -x 'knockout' -x 'jquery' www/js/table.js > www/js/table.bundle.js"
  },
  "homepage": "https://github.com/devilesk/dota-hero-calculator#readme",
  "devDependencies": {
    "bootstrap": "^3.3.7",
    "brfs": "^1.4.3",
    "browserify": "^13.1.0",
    "browserify-replace": "^0.9.0",
    "browserify-shim": "^3.8.12",
    "chart.js": "^1.1.1",
    "del": "^2.2.1",
    "dota-hero-calculator-library": "^0.5.1",
    "git-rev-sync": "^1.6.0",
    "gulp": "^3.9.1",
    "gulp-chmod": "^1.3.0",
    "gulp-clean-css": "^2.0.10",
    "gulp-concat": "^2.6.0",
    "gulp-imagemin": "^3.0.1",
    "gulp-preprocess": "^2.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rollbar": "^0.1.3",
    "gulp-sequence": "^0.4.5",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "jquery": "^3.1.0",
    "jquery-ui": "^1.12.0",
    "knockout": "^3.4.0",
    "knockout-mapping": "^2.6.0",
    "request": "^2.74.0",
    "rollbar-browser": "^1.9.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {},
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "knockout": "global:ko",
    "jquery": "global:$",
    "jquery-ui": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  }
}
